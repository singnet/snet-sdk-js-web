{
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": "./src",
    "exclude": [
      "./src/payment_channel_state_service_grpc_pb.js",
      "./src/payment_channel_state_service_pb.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
